#include<stdio.h>
int main()
{
	int a, b;
	printf("Enter the first number: \n");
	scanf("%d",&a);
	printf("Enter the second number: \n");
	scanf("%d",&b);
	printf("Numbers before swapping a=%d, b=%d\n",a,b);
	//Swapping code
	a=a+b;
	b=a-b;
	a=a-b;
	printf("Numbers after swapping a=%d, b=%d",a,b);
	return 0;
}
